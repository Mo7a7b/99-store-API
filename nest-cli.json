{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      { "include": "templates/*.html", "outDir": "dist/src" },
      { "include": "../keys/*.pem", "outDir": "dist/keys" }
    ]
  }
}
